    <Authors>Christoffer Lette</Authors>
    <Company>Christoffer Lette</Company>
    <Description>Command Line Arguments Parser Demo</Description>
    <Copyright>Copyright Â© Christoffer Lette 2014-</Copyright>
    <PackageLicenseUrl>https://github.com/Lette/Lette.CmdLineArgsParser/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Lette/Lette.CmdLineArgsParser</PackageProjectUrl>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Version>0.1.0</Version>
    <Product>Lette.CmdLineArgsParser</Product>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn />
